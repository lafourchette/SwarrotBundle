{
    "name": "lafourchette/swarrot-bundle",
    "description": "SwarrotBundle",
    "keywords": ["swarrot","bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.lafourchette.com"
        },
        {
            "type": "git",
            "url": "https://github.com/lafourchette/swarrot.git"
        }
    ],
    "require": {
        "php": ">=5.3",
        "lafourchette/swarrot": "dev-master@dev",
        "psr/log": "~1.0",
        "symfony/framework-bundle": "~2.4",
        "symfony/console": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "suggest": {
        "videlalvaro/php-amqplib": "Required if using amqp_lib"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Swarrot\\SwarrotBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
